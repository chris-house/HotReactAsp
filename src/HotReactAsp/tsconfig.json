{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "outDir": "./ClientApp/compiledjs/"

  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
  //,"compileOnSave": true
}